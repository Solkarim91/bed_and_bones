<h1>My Bookings</h1>

<ul>
  <% @bookings.each do |booking| %>
    <li>
      <%= booking.pet.name %> |
      <%= booking.pet.species.name %> |
      From: <%= booking.start_date %>
      To: <%= booking.end_date %>
    </li>
    <%= link_to 'View', pet_booking_path(booking.pet, booking) %> |
    <%= link_to 'Delete', booking_path(booking), method: :delete %>
  <% end %>
  <%= link_to 'Back', pets_path %>
</ul>
